{
  "name": "@signifly/travy",
  "version": "0.0.0-rc.6",
  "license": "MIT",
  "main": "dist/lib.common.js",
  "repository": "github:signifly/travy",
  "scripts": {
    "release": "docker-compose run --rm app sh -c 'yarn && yarn test && yarn build-lib' && np --yolo",
    "build-lib": "vue-cli-service build --target lib --formats commonjs --name lib src/lib.js",
    "build-app": "vue-cli-service build --modern",
    "jest": "vue-cli-service test:unit --verbose",
    "dev": "vue-cli-service serve --port 3000",
    "inspect": "vue-cli-service inspect",
    "test": "yarn lint && yarn jest",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "element-ui": "2.6.1",
    "emojilib": "2.4.0",
    "lodash": "4.17.11",
    "normalize.css": "8.0.1",
    "object-to-formdata": "1.6.3",
    "qs": "6.6.0",
    "semaphore-async-await": "1.5.1",
    "simplemde-theme-base": "0.2.1",
    "vue": "2.6.7",
    "vue-router": "3.0.2",
    "vue-shortkey": "3.1.6",
    "vue-simplemde": "0.5.1",
    "vuedraggable": "2.17.0",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.4.1",
    "@vue/cli-plugin-eslint": "3.4.1",
    "@vue/cli-plugin-unit-jest": "3.4.1",
    "@vue/cli-service": "3.4.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "babel-plugin-component": "1.1.1",
    "babel-plugin-lodash": "3.3.4",
    "eslint-plugin-jest": "22.3.0",
    "html-loader": "0.5.5",
    "markdown-loader": "5.0.0",
    "sass": "1.17.2",
    "sass-loader": "7.1.0",
    "svg-inline-loader": "0.8.0",
    "vue-cli-plugin-element": "1.0.1",
    "vue-template-compiler": "2.6.7"
  }
}
